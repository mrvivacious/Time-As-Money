{
    "manifest_version": 3,
  
    "name": "Time As Money", 
    "description": "Convert prices to hourly rates",
    "version": "0.69",
    "author": "Vivek Bhookya",
  
    "action": {
        "default_popup": "popup.html",
        "default_title": "Time As Money"
      },
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["js.js"],
        "run_at": "document_end"
      }
    ],
  
    "permissions": [
      "activeTab",
      "storage"
    ]
  }